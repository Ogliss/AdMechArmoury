	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeOG_Ammo_BoltStd</defName>
		<label>make standard Bolt cartridge x200</label>
		<description>Craft 200 standard Bolts.</description>
		<jobString>Making standard Bolts.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>FSX</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>Steel</li>
				<li>FSX</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<OG_Ammo_BoltStd>200</OG_Ammo_BoltStd>
		</products>
		<researchPrerequisites>
			<li>OG_Imperial_Tech_Weapons_Bolt</li>
		</researchPrerequisites>
	</RecipeDef>


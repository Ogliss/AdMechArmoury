<Defs>
	<!-- ========================= Offico Assasain ========================== -->
	<!-- ========================= Synth Skin ========================== -->
	<ThingDef ParentName="OG_Imperial_Apparel_Armor">
		<defName>OGOA_Apparel_SynthSkin</defName>
		<label>synth-skin</label>
		<description>.</description>
		<graphicData>
			<texPath>Things/Item/Apparel/Imperial/OA/SynthSkin/__Icon</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<color>(74,97,71)</color>
			<drawSize>1.00</drawSize>
		</graphicData>
		<recipeMaker>
			<researchPrerequisites>
				<li>OG_Assassinorum_Tech_Base</li>
			</researchPrerequisites>
			<skillRequirements>
				<Crafting>5</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<Cloth>50</Cloth>
			<Steel>70</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<statBases>
			<MaxHitPoints>260</MaxHitPoints>
			<WorkToMake>14000</WorkToMake>
			<Mass>4</Mass>
			<Flammability>0.6</Flammability>
			<ArmorRating_Sharp>0.625</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.625</ArmorRating_Blunt>
			<ArmorRating_Heat>0.625</ArmorRating_Heat>
			<Insulation_Cold>10.4</Insulation_Cold> <!-- like a cloth jacket -->
			<Insulation_Heat>3</Insulation_Heat>
			<EquipDelay>6</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.04</MoveSpeed>
			<WorkSpeedGlobal>-0.020</WorkSpeedGlobal>
		</equippedStatOffsets>
		<apparel>
			<useWornGraphicMask>true</useWornGraphicMask>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Item/Apparel/Imperial/OA/SynthSkin/SynthSkin</wornGraphicPath>
			<layers>
				<li>Middle</li>
			</layers>
			<tags>
				<li>OGOA_Synthskin</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
	</ThingDef>


<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<AdeptusMechanicus.CultureDef>
		<defName>OG_Imperial_Militarum_Spacer</defName>
		<label>militarum</label>
		<description>A broad collection of cultures common among members of the Astra Militarum.</description>
		<ideoNameMaker MayRequire="Ludeon.RimWorld.Ideology">NamerIdeo_Militarum_Spacer</ideoNameMaker>
		<deityNameMaker MayRequire="Ludeon.RimWorld.Ideology">NamerDeity_EmperorOfMan</deityNameMaker>
		<deityTypeMaker MayRequire="Ludeon.RimWorld.Ideology">DeityTypeMaker_EmperorOfMan</deityTypeMaker>
		<leaderTitleMaker MayRequire="Ludeon.RimWorld.Ideology">LeaderTitleMaker_Militarum_Spacer</leaderTitleMaker>
		<festivalNameMaker MayRequire="Ludeon.RimWorld.Ideology">NamerFestivalAstropolitan</festivalNameMaker>
		<pawnNameMaker>OG_Militarum_NamerPerson</pawnNameMaker>
		<pawnNameMakerFemale>OG_Militarum_NamerPersonFemale</pawnNameMakerFemale>
		<allowedPlaceTags>
			<li>OriginSpacer</li>
			<!--li>ImperialOriginSpacer</li-->
		</allowedPlaceTags>
		<iconPath>Icons/Imperial/Militarum/Icon_Adeptus_Militarum</iconPath>
		<iconColor>(0.8, 0.8, 0.8)</iconColor>
		<deities>
			<requiredDeities>
				<li>OG_EmperorOfMan</li>
			</requiredDeities>
		</deities>
		<preferredApparelTags>
			<li>ImperialApparel</li>
		</preferredApparelTags>
		<thingStyleCategories>
			<li>
				<category>OG_Imperial</category>
				<priority>1</priority>
			</li>
		</thingStyleCategories>
		<styleItemTags>
			<li>
				<tag>Urban</tag>
				<baseWeight>1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>NoBeard</tag>
				<baseWeight>10</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>BeardRural</tag>
				<baseWeight>0.1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>BeardUrban</tag>
				<baseWeight>0.2</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
			<li>
				<tag>NoTattoo</tag>
				<baseWeight>1</baseWeight>
				<weightFactor>1</weightFactor>
			</li>
		</styleItemTags>
	</AdeptusMechanicus.CultureDef>

	

<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<AdeptusMechanicus.BackstoryDef ParentName="Imperial_Base_Adult_Backstory">
		<defName>OG_Imperial_Inquisiton_Inquisitor_Neophyte</defName>
		<title>imperial inquisitor neophyte</title>
		<titleShort>inquisitor</titleShort>
		<baseDesc>[PAWN_nameDef] is a neophyte Inquisitor attached to a Rimworld expedition. Whether he/she will choose the Radical Path, using Xeno tech, or the Puritan Path, purging all non-human filth from the universe, is yet to be seen.</baseDesc>
		<slot>Adulthood</slot>
		<!--	+3 to melee, shooting, social, and intellect -->
		<workDisables>
			<!--
			<li>Intellectual</li>
			<li>Caring</li>
			<li>Animals</li>
			<li>ManualSkilled</li>
			<li>Artistic</li>
			<li>Firefighting</li>
			-->
		</workDisables>
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
		<forcedTraitsChance>
			<li MayRequire="QX.AstraMilitarum">
				<defName>IGTraitAstraMilitarum</defName>
				<chance>5</chance>
			</li>
		</forcedTraitsChance>
		<disallowedTraits />
		<skillGains>
			<li>
				<key>Melee</key>
				<value>3</value>
			</li>
			<li>
				<key>Shooting</key>
				<value>3</value>
			</li>
			<li>
				<key>Social</key>
				<value>3</value>
			</li>
			<li>
				<key>Intellectual</key>
				<value>3</value>
			</li>
		</skillGains>
		<spawnCategories>
			<li>Imperium_Standard</li>
			<li>Imperium_Schola_Cadet</li>
			<li>Imperial_Inquisitor_Neophyte</li>
			<li>Imperial_Inquisitor</li>
		</spawnCategories>
	</AdeptusMechanicus.BackstoryDef>

	<AdeptusMechanicus.BackstoryDef ParentName="Imperial_Base_Adult_Backstory">
		<defName>OG_Imperial_Inquisiton_Inquisitor</defName>
		<title>imperial inquisitor</title>
		<titleShort>inquisitor</titleShort>
		<baseDesc>[PAWN_nameDef] served the Emperor of Mankind by searching for that which would endanger the Imperium, and when [PAWN_nameDef] found those dangers, it was their duty to stop them.	 </baseDesc>
		<slot>Adulthood</slot>
		<workDisables>
			<li>ManualDumb</li>	
			<li>Crafting</li>
			<li>Cooking</li> 
			<li>PlantWork</li> 
			<li>Mining</li>
		</workDisables>
		<requiredWorkTags>
			<li>Violent</li>
			<li>Social</li>
		</requiredWorkTags>
		<forcedTraits>
			<IGTraitInquisition MayRequire="QX.AstraMilitarum">0</IGTraitInquisition>
		</forcedTraits>
		<forcedTraitsChance>
			<li MayRequire="erdelf.HumanoidAlienRaces">
				<defName>Xenophobia</defName>
				<chance>40</chance>
				<degree>1</degree>
			</li>
			<li>
				<defName>Beauty</defName>
				<chance>45</chance>
				<degree>2</degree>
			</li>
		</forcedTraitsChance>
		<disallowedTraits />
		<skillGains>
			<li>
				<key>Shooting</key>
				<value>6</value>
			</li>
			<li>
				<key>Melee</key>
				<value>3</value>
			</li>
			<li>
				<key>Intellectual</key>
				<value>6</value>
			</li>
			<li>
				<key>Artistic</key>
				<value>-4</value>
			</li>
		</skillGains>
		<spawnCategories>
			<li>Imperium_Standard</li>
			<li>Imperium_Schola_Cadet</li>	
			<li>Imperium_Inquisitor</li>
		</spawnCategories>
	</AdeptusMechanicus.BackstoryDef>
	

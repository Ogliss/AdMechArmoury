<?xml version="1.0" encoding="utf-8"?>

<Defs>

	<!--==================== Imperial - Bolt Sound =========================-->
	<SoundDef>
		<defName>BoltSound</defName>
		<context>MapOnly</context>
		<eventNames />  
		<maxSimultaneous>10</maxSimultaneous>  
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
					<clipPath>Weapons/Ranged/Bolt_Shot</clipPath>
					</li>
				</grains>      
				<volumeRange>
					<min>55.0</min>        
					<max>100.0</max>
				</volumeRange>      
				<pitchRange>
					<min>0.9</min>        
					<max>1.0</max>
				</pitchRange>      
				<sustainLoop>false</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>	

<!-- ======================== Imperial - Frag Grenade Ability ================================== -->
	<ThingDef ParentName="BaseGrenadeAbility">
		<defName>Projectile_OGIGrenadeFrag</defName>
		<graphicData>
			<texPath>Things/Projectile/ImpFragGrenade</texPath>
		</graphicData>
		<projectile>
			<damageDef>OGGrenadeFrag</damageDef>
			<!-- <damageAmountBase>20</damageAmountBase> -->
			<armorPenetrationBase>0.16</armorPenetrationBase>
			<explosionRadius>3</explosionRadius>
			<explosionDelay>100</explosionDelay>
			<speed>12</speed>
			<flyOverhead>false</flyOverhead>
		</projectile>
	</ThingDef>

	<AbilityUser.AbilityDef ParentName="BaseAbility">
        <defName>OGI_Ability_ThrowFrag</defName>
        <label>Throw Frag</label>
        <uiIconPath>Things/Projectile/ImpFragGrenade</uiIconPath>
        <description>Throws a Frag Grenade.</description>
        <MainVerb Class="AdeptusMechanicus.VerbProperties_EquipmentAbility">
            <verbClass>AdeptusMechanicus.Verb_UseEquipment</verbClass>
            <AlwaysHits>false</AlwaysHits>
            <tooltipShowProjectileDamage>true</tooltipShowProjectileDamage>
            <tooltipShowExtraDamages>false</tooltipShowExtraDamages>
            <tooltipShowHediffsToApply>false</tooltipShowHediffsToApply>
            <tooltipShowMentalStatesToApply>false</tooltipShowMentalStatesToApply>
            <hasStandardCommand>true</hasStandardCommand>
			<ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
			<defaultProjectile>Projectile_OGIGrenadeFrag</defaultProjectile>
            <abilityDef>OGI_Ability_ThrowFrag</abilityDef>
            <warmupTime>2.4</warmupTime>
            <SecondsToRecharge>15.0</SecondsToRecharge>
            <burstShotCount>1</burstShotCount>
            <ticksBetweenBurstShots>2</ticksBetweenBurstShots>
			<range>12.9</range>
			<forcedMissRadius>1.9</forcedMissRadius>
            <soundCast>ThrowGrenade</soundCast>
            <muzzleFlashScale>0</muzzleFlashScale>
            <isPrimary>false</isPrimary>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
        </MainVerb>
	</AbilityUser.AbilityDef>
 

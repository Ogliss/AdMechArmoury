	<!--==================== Chainsword interact =========================-->
	<SoundDef>
		<defName>Interact_Chainsword</defName>
		<context>MapOnly</context>
		<eventNames />  
		<maxSimultaneous>1</maxSimultaneous>  
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
					<clipFolderPath>Weapons/Melee/ChainswordInteract</clipFolderPath>
					</li>
				</grains>      
				<volumeRange>
					<min>40.0</min>        
					<max>60.0</max>
				</volumeRange>      
				<pitchRange>
				  <min>0.9</min>        
				  <max>1.0</max>
				</pitchRange>      
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>


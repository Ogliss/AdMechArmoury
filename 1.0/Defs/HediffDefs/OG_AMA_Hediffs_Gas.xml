          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.4</offset>
          </li>
        </capMods>
      </li>
      <li>
        <painFactor>0.20</painFactor>
		<hungerRateFactor>0.5</hungerRateFactor>
		<minSeverity>0.70</minSeverity>
        <label>passed out</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.1</setMax>	
          </li>
        </capMods>
      </li>
	  <li>
        <painFactor>0.20</painFactor>
		<hungerRateFactor>0.5</hungerRateFactor>
		<minSeverity>0.95</minSeverity>
        <label>comatose</label>
        <lifeThreatening>true</lifeThreatening>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.1</setMax>	
          </li>
        </capMods>
      </li>
    </stages>

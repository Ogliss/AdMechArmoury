  <!-- ========================= Imperial Guard - Light Flak ========================== -->
  <ThingDef ParentName="OGIArmorMakeable">
    <defName>OGIG_Apparel_LightFlakArmour</defName>
    <label>Light flak armour</label>
    <description>A tough fabric jacket with armor plates sewn in. While it's not as good as solid armor, it offers effective protection against all manner of attacks while remaining quite comfortable.</description>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/Apparel/Imperial/IG/IG_Cadian_Chestplate/IG_Cadian_Chestplate</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.00</drawSize>
    </graphicData>
    <costList>
      <Cloth>50</Cloth>
      <Steel>70</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <statBases>
      <MaxHitPoints>260</MaxHitPoints>
      <WorkToMake>14000</WorkToMake>
      <Mass>7</Mass>
      <Flammability>0.6</Flammability>
      <ArmorRating_Sharp>0.55</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.16</ArmorRating_Blunt>
      <ArmorRating_Heat>0.20</ArmorRating_Heat>
      <Insulation_Cold>10.4</Insulation_Cold> <!-- like a cloth jacket -->
      <Insulation_Heat>3</Insulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.05</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Item/Apparel/Imperial/IG/IG_Cadian_Chestplate/IG_Cadian_Chestplate</wornGraphicPath>
      <layers>
        <li>Middle</li>
        <li>Shell</li>
      </layers>
      <tags>
        <li>OGImperialGuard</li>
        <li>OGCadian</li>
        <li>OGIGArmour</li>
        <li>OGCadianArmour</li>
        <li>OGIGFlak</li>
        <li>OGCadianFlak</li>
        <li>OGIGFlakChest</li>
        <li>OGCadianFlakChest</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel><!--

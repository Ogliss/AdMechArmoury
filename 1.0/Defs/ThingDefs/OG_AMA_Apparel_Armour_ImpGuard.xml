  <!-- ========================= Imperial Guard - Flak Armour ========================== -->
  <ThingDef ParentName="OGIArmorMakeable">
    <defName>OGIG_Apparel_FlakArmour</defName>
    <thingClass>AdeptusMechanicus.Apparel_VisibleAccessory</thingClass>
    <modExtensions>
      <li Class="AdeptusMechanicus.VisibleAccessoryDefExtension">
        <order>1</order>
        <northtop>false</northtop>
					<NorthOffset>-0.0025</NorthOffset>
					<SouthOffset>-0.0025</SouthOffset>
					<EastOffset>-0.0025</EastOffset>
					<WestOffset>-0.0025</WestOffset>
      </li>
    </modExtensions>
    <label>imperial guard Flak Armor</label>
    <description>Standard Issue Flak Armor for the Emperor's Hammer, it offers effective protection against all manner of attacks while remaining quite light.</description>
    <techLevel>Spacer</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/Apparel/Imperial/IG/IG_ArmorFC/IG_Armor</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <Mass>4</Mass>
      <WorkToMake>24000</WorkToMake>
      <MaxHitPoints>200</MaxHitPoints>
      <MarketValue>500</MarketValue>
      <ArmorRating_Sharp>1.00</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.36</ArmorRating_Blunt>
      <ArmorRating_Heat>0.30</ArmorRating_Heat>
      <Insulation_Cold>10</Insulation_Cold>
      <Insulation_Heat>10</Insulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.10</MoveSpeed>
      <WorkSpeedGlobal>0.0</WorkSpeedGlobal>
    </equippedStatOffsets>
    <costList>
      <Steel>1</Steel>
      <Plasteel>1</Plasteel>
      <Hyperweave>1</Hyperweave>
    </costList>
	<generateCommonality>2</generateCommonality>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Item/Apparel/Imperial/IG/IG_ArmorFC/IG_Armor</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>OGImperialGuard</li>
        <li>OGIGArmour</li>
        <li>OGIGFlak</li>
        <li>OGIGFlakChest</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <comps>
      <li Class="AdeptusMechanicus.CompProperties_PauldronDrawer">
        <compClass>AdeptusMechanicus.CompPauldronDrawer</compClass>
			<PauldronEntries>
				<li>
					<shoulderPadType>Both</shoulderPadType>
					<shaderType>CutoutComplex</shaderType>
					<padTexPath>Things/Item/Apparel/Imperial/IG/IG_Armor/Pauldrons/Flak_Pauldrons</padTexPath>
					<commonality>10</commonality>
				</li>
			</PauldronEntries>
					<NorthOffset>0.15</NorthOffset>
					<SouthOffset>0.15</SouthOffset>
					<EastOffset>0.15</EastOffset>
					<WestOffset>0.15</WestOffset>
      </li>
    </comps>
	
	<!--
	
	-->
    <colorGenerator Class="ColorGenerator_Options">
      <options>
		<li>
			<weight>10</weight>
			<max>(0.349,0.541,0.361)</max>
			<min>(0.263,0.376,0.251)</min>
		</li>
      </options>
    </colorGenerator>
  </ThingDef>


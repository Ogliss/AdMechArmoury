	<ThingDef Class="CombatExtended.AmmoDef" ParentName="OGPlasmaBase">
		<defName>OG_Ammo_IPlasma</defName>
		<label>hydrogen flask (Imperial)</label>
		<description>A flask of hydrogen fuel, can be used in standard imperial and chaos plasma weaponry</description>
		<graphicData>
			<texPath>Things/Ammo/FuelCell/Thermobaric</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.95</MarketValue>
		</statBases>
		<ammoClass>OGPlasmaAC</ammoClass>
	</ThingDef>

	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeOG_Ammo_IPlasma</defName>
		<label>make Imperial hydrogen flask x60</label>
		<description>Craft 60 Imperial hydrogen flasks.</description>
		<jobString>Making Imperial hydrogen flasks.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Prometheum</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>FSX</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ComponentIndustrial</li>
				<li>Prometheum</li>
				<li>FSX</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<OG_Ammo_IPlasma>60</OG_Ammo_IPlasma>
		</products>
		<researchPrerequisite>OG_Common_Tech_Weapons_Plasma</researchPrerequisite>
	</RecipeDef>


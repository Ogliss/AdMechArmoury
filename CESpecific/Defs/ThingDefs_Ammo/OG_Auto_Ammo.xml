	<!-- ================== Craft Autocannon Shells ================== -->
	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeOG_Ammo_Autocannon</defName>
		<label>make AutoCannon cartridge x60</label>
		<description>Craft 60 AutoCannon cartridges.</description>
		<jobString>Making AutoCannon cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
					<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
					<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<OG_Ammo_Autocannon>60</OG_Ammo_Autocannon>
		</products>
		<researchPrerequisite>OG_Common_Tech_Weapons_Heavy</researchPrerequisite>
	</RecipeDef>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="OG_AmmoBase_AutoWeapon">
		<defName>OG_Ammo_Autocannon</defName>
		<label>Autocannon shell</label>
		<graphicData>
			<texPath>Things/Ammo/HighCaliber/HE</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.95</MarketValue>
		</statBases>
		<ammoClass>FullMetalJacket</ammoClass>
	</ThingDef>


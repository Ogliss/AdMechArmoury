<?xml version="1.0" encoding="utf-8"?>

<Defs>

	<!--==================== Eldar - Starcannon =========================-->
	<SoundDef>
		<defName>OG_Starcannon_Sound</defName>
		<context>MapOnly</context>
		<eventNames />  
		<maxSimultaneous>1</maxSimultaneous>  
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
					<clipFolderPath>Weapons/Ranged/Starcannon_Shot</clipFolderPath>
					</li>
				</grains>      
				<volumeRange>
					<min>15.0</min>        
					<max>25.0</max>
				</volumeRange>      
				<pitchRange>
					<min>0.9</min>        
					<max>1.0</max>
				</pitchRange>      
				<sustainLoop>false</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>
	
	

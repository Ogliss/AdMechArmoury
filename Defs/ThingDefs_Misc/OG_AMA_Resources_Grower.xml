<Defs>
	<ThingDef ParentName="ResourceBase">
		<defName>OG_NutrientSolution</defName>
		<label>nutrient solution</label>
		<description>A mixture of water, amino acids, fats &amp; vitamins for growing biological matter.</description>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>Things/Item/Resource/Imperial/NutrientSolution</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.85</drawSize>
		</graphicData>
		<stackLimit>150</stackLimit>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<MarketValue>2.1</MarketValue>
			<Mass>0.05</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
	</ThingDef>

	<ThingDef ParentName="ResourceBase">
		<defName>OG_ProteinMash</defName>
		<label>protein mash</label>
		<description>Protein from different sources mashed together into a disgusting mash. Perfect for growing organs and living beings from.</description>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>Things/Item/Resource/Imperial/ProteinMash</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.85</drawSize>
		</graphicData>
		<stackLimit>150</stackLimit>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<MarketValue>2.1</MarketValue>
			<Mass>0.05</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
	</ThingDef>
	

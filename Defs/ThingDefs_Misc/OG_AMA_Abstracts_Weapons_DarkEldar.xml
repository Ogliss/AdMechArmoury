	<!-- ==================== Mod - Base Dark Eldar Melee Weapon Sharp Template ========================= -->
	<ThingDef Name="OG_DarkEldar_MeleeWeapon_Sharp" ParentName="OG_DarkEldar_MeleeWeapon_Makeable" Abstract="True">
		<recipeMaker>
			<defaultIngredientFilter>
				<categories>
					<li>Root</li>
				</categories>
				<disallowedThingDefs>
					<li>Uranium</li>
					<li>Silver</li>
					<li>Gold</li>
				</disallowedThingDefs>
			</defaultIngredientFilter>
		</recipeMaker>
		</ThingDef>


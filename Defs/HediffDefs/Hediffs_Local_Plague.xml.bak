<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<HediffDef ParentName="InfectionBase">
		<defName>Plague</defName>
		<label>plague</label>
		<makesSickThought>true</makesSickThought>
		<lethalSeverity>1</lethalSeverity>
		<tendable>true</tendable>
		<comps>
			<li Class="HediffCompProperties_TendDuration">
				<baseTendDurationHours>36</baseTendDurationHours>
				<severityPerDayTended>-0.38</severityPerDayTended>
			</li>
			<li Class="HediffCompProperties_Immunizable">
				<immunityPerDayNotSick>-0.02</immunityPerDayNotSick>
				<immunityPerDaySick>0.3</immunityPerDaySick>
				<severityPerDayNotImmune>0.4</severityPerDayNotImmune>
				<severityPerDayImmune>-0.2</severityPerDayImmune>
				<severityPerDayNotImmuneRandomFactor>
					<min>0.85</min>
					<max>1.15</max>
				</severityPerDayNotImmuneRandomFactor>
			</li>
		</comps>
		<stages>
			<li>
				<label>initial</label>
			</li>
			<li>
				<minSeverity>0.4</minSeverity>
				<label>minor</label>
				<painOffset>0.10</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.05</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
			</li>
			<li>
				<minSeverity>0.5</minSeverity>
				<label>moderate</label>
				<painOffset>0.25</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.10</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.10</offset>
					</li>
					<li>
						<capacity>Hearing</capacity>
						<offset>-0.10</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>BenignGrowth</hediff>
						<mtbDays>5</mtbDays>
						<canAffectAnyLivePart>true</canAffectAnyLivePart>
					</li>
				</hediffGivers>
			</li>
			<li>
				<minSeverity>0.6</minSeverity>
				<label>major</label>
				<painOffset>0.40</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Hearing</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.10</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>BenignGrowth</hediff>
						<mtbDays>2.5</mtbDays>
						<canAffectAnyLivePart>true</canAffectAnyLivePart>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>EncapsulatedPlague</hediff>
						<mtbDays>15</mtbDays>
						<partsToAffect>
							<li>Spine</li>
							<li>Pelvis</li>
							<li>Femur</li>
							<li>Humerus</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<minSeverity>0.7</minSeverity>
				<label>severe</label>
				<painOffset>0.55</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.35</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.35</offset>
					</li>
					<li>
						<capacity>Hearing</capacity>
						<offset>-0.30</offset>
					</li>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.10</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>BenignGrowth</hediff>
						<mtbDays>1</mtbDays>
						<canAffectAnyLivePart>true</canAffectAnyLivePart>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>EncapsulatedPlague</hediff>
						<mtbDays>10</mtbDays>
						<partsToAffect>
							<li>Brain</li>
							<li>Sternum</li>
							<li>Clavicle</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>SecondaryDamage</hediff>
						<mtbDays>60</mtbDays>
						<partsToAffect>
							<li>Ear</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>ComaForever</hediff>
						<mtbDays>360</mtbDays>
						<partsToAffect>
							<li>Brain</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>ComaTimed</hediff>
						<mtbDays>180</mtbDays>
						<partsToAffect>
							<li>Brain</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<minSeverity>0.8</minSeverity>
				<label>extreme</label>
				<painOffset>0.70</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.55</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.55</offset>
					</li>
					<li>
						<capacity>Hearing</capacity>
						<offset>-0.40</offset>
					</li>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.30</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Sight</capacity>
						<offset>-0.10</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>BenignGrowth</hediff>
						<mtbDays>0.5</mtbDays>
						<canAffectAnyLivePart>true</canAffectAnyLivePart>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>EncapsulatedPlague</hediff>
						<mtbDays>5</mtbDays>
						<partsToAffect>
							<li>Tibia</li>
							<li>Radius</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>SecondaryDamage</hediff>
						<mtbDays>45</mtbDays>
						<partsToAffect>
							<li>Jaw</li>
							<li>Ear</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>ComaForever</hediff>
						<mtbDays>180</mtbDays>
						<partsToAffect>
							<li>Brain</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>ComaTimed</hediff>
						<mtbDays>90</mtbDays>
						<partsToAffect>
							<li>Brain</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<minSeverity>0.9</minSeverity>
				<label>fatal</label>
				<lifeThreatening>true</lifeThreatening>
				<painOffset>0.85</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.80</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.80</offset>
					</li>
					<li>
						<capacity>Hearing</capacity>
						<offset>-0.50</offset>
					</li>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.40</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.30</offset>
					</li>
					<li>
						<capacity>Sight</capacity>
						<offset>-0.20</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>BenignGrowth</hediff>
						<mtbDays>0.25</mtbDays>
						<canAffectAnyLivePart>true</canAffectAnyLivePart>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>EncapsulatedPlague</hediff>
						<mtbDays>2.5</mtbDays>
						<partsToAffect>
							<li>Spine</li>
							<li>Pelvis</li>
							<li>Femur</li>
							<li>Humerus</li>
							<li>Brain</li>
							<li>Sternum</li>
							<li>Clavicle</li>
							<li>Tibia</li>
							<li>Radius</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>SecondaryDamage</hediff>
						<mtbDays>30</mtbDays>
						<partsToAffect>
							<li>Jaw</li>
							<li>Ear</li>
							<li>Lung</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>ComaForever</hediff>
						<mtbDays>90</mtbDays>
						<partsToAffect>
							<li>Brain</li>
						</partsToAffect>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>ComaTimed</hediff>
						<mtbDays>45</mtbDays>
						<partsToAffect>
							<li>Brain</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
		</stages>
	</HediffDef>

	<HediffDef ParentName="DiseaseBase">
		<defName>EncapsulatedPlague</defName>
		<label>encapsulated pathogen</label>
		<stages>
			<li>
				<partEfficiencyOffset>-0.1</partEfficiencyOffset>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>Plague</hediff>
						<mtbDays>90</mtbDays>
					</li>
					<li Class="HediffGiver_Random">
						<hediff>BenignGrowth</hediff>
						<mtbDays>45</mtbDays>
						<canAffectAnyLivePart>true</canAffectAnyLivePart>
					</li>
				</hediffGivers>
			</li>
		</stages>
	</HediffDef>

</Defs>
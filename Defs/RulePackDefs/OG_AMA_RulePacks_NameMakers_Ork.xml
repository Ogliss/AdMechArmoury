  <!-- ========================= Orkoid Name Utility ========================= -->
	<RulePackDef>
		<defName>OG_Orkoid_NameUtility</defName>
		<rulePack>
			<rulesStrings>
				<!--li>WordOrk(p=4)->[SylB][SylB]</li>
				<li>WordOrk(p=2)->[SylB][SylB][SylB]</li>
				<li>WordOrk(p=0.2)->[SylB][SylB][SylB][SylB]</li-->
				
				<li>NameOrk(p=8)->[OrkFirst]</li>
				<li>NameOrk(p=4)->[OrkFirst] [OrkSecond]</li>
				<li>NameOrk(p=0.2)->[Ork_First] '[TitlePost]' [Ork_First_B]</li>
				
				<li>OrkFirst(p=4)->[Ork_First_A][Ork_First_B]</li>
				<li>OrkFirst(p=1)->[Ork_First]</li>
				<li>OrkSecond(p=4)->[Ork_Second_A][Ork_Second_B]</li>
				<li>OrkSecond(p=1)->[Ork_Second]</li>
				
				<li>NameGrot(p=15)->[GrotFirst]</li>
				<li>NameGrot(p=10)->[GrotFirst] [GrotSecond]</li>
				<li>NameGrot(p=1)->[GrotFirst] '[Grot_Nick]' [GrotSecond]</li>
				
				<li>GrotFirst(p=4)->[Grot_First_A][Grot_First_B]</li>
				<li>GrotSecond(p=4)->[Grot_Second_A][Grot_Second_B]</li>
				
				<li>NameSquig->[Squig_A][Squig_B]</li>
				
				<li>NameSnot(p=10)->Snot[Ork_First_B]</li>
				<li>NameSnot(p=7)->Snot[Ork_Second_B]</li>
				<li>NameSnot(p=5)->[Ork_First_A]snot</li>
				<li>NameSnot(p=1)->[Ork_Second_A]snot</li>
				
				<li>NameSnot(p=10)->Snot[Grot_First_B]</li>
				<li>NameSnot(p=7)->Snot[Grot_Second_B]</li>
				<li>NameSnot(p=5)->[Grot_First_A]snot</li>
				<li>NameSnot(p=1)->[Grot_Second_A]snot</li>
				
				<li>Klan(p=3)->[Klan_A] [Klan_B]</li>
			</rulesStrings>
			<rulesFiles>
				<li>Klan_A->NameBanks/OG_Ork_Faction_Names_A</li>
				<li>Klan_B->NameBanks/OG_Ork_Faction_Names_B</li>
				<li>Klan_C->NameBanks/OG_Ork_Faction_Names_C</li>
				<li>Kultrue->NameBanks/OG_Ork_Kulture</li>
				
				<li>Ork_Outpost->NameBanks/OG_Ork_BaseNames</li>
				
				<li>Ork_First_A->NameBanks/OG_Ork_Names_First_A</li>
				<li>Ork_First_B->NameBanks/OG_Ork_Names_First_B</li>
				<li>Ork_First->NameBanks/OG_Ork_Names_First</li>
				<li>Ork_Second_A->NameBanks/OG_Ork_Names_Last_A</li>
				<li>Ork_Second_B->NameBanks/OG_Ork_Names_Last_B</li>
				<li>Ork_Second->NameBanks/OG_Ork_Names_Last</li>
				<!--li>Ork_Nick->NameBanks/OG_Ork_Names_Nick</li-->
				
				<li>Ork_Warboss->NameBanks/OG_Ork_Leader_Names</li>
				<li>Ork_Title_Pre->NameBanks/OG_Ork_Leader_Titles_Prefix</li>
				<li>Ork_Title_Post->NameBanks/OG_Ork_Leader_Titles_Postfix</li>
				
				<li>Grot_First_A->NameBanks/OG_Grot_Names_First_A</li>
				<li>Grot_First_B->NameBanks/OG_Grot_Names_First_B</li>
				<li>Grot_Second_A->NameBanks/OG_Grot_Names_Second_A</li>
				<li>Grot_Second_B->NameBanks/OG_Grot_Names_Second_B</li>
				<li>Grot_Nick->NameBanks/OG_Grot_Names_Nick</li>
				
				<li>Squig->NameBanks/OG_Ork_Names_Last</li>
				<li>Squig_A->NameBanks/OG_Squig_Names_A</li>
				<li>Squig_B->NameBanks/OG_Squig_Names_B</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>

</Defs>
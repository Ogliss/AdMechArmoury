	<!-- ========================= Orkoid Name Utility ========================= -->
	<RulePackDef>
		<defName>OG_Orkoid_NameUtility</defName>
		<rulePack>
			<rulesStrings>
				<!--li>WordOrk(p=4)->[SylB][SylB]</li>
				<li>WordOrk(p=2)->[SylB][SylB][SylB]</li>
				<li>WordOrk(p=0.2)->[SylB][SylB][SylB][SylB]</li-->
				
				<li>NameOrk(p=8)->[OrkFirst]</li>
				<li>NameOrk(p=4)->[OrkFirst] [OrkSecond]</li>
				<li>NameOrk(p=0.2)->[Ork_First] '[TitlePost]' [Ork_First_B]</li>
				
				<li>OrkFirst(p=4)->[Ork_First_A][Ork_First_B]</li>
				<li>OrkFirst(p=1)->[Ork_First]</li>
				<li>OrkSecond(p=4)->[Ork_Second_A][Ork_Second_B]</li>
				<li>OrkSecond(p=1)->[Ork_Second]</li>
				
				<li>NameGrot(p=15)->[GrotFirst]</li>
				<li>NameGrot(p=10)->[GrotFirst] [GrotSecond]</li>
				<li>NameGrot(p=1)->[GrotFirst] '[Grot_Nick]' [GrotSecond]</li>
				
				<li>GrotFirst(p=4)->[Grot_First_A][Grot_First_B]</li>
				<li>GrotSecond(p=4)->[Grot_Second_A][Grot_Second_B]</li>
				
				<li>NameSquig->[Squig_A][Squig_B]</li>
				
				<li>NameSnot(p=10)->Snot[Ork_First_B]</li>
				<li>NameSnot(p=7)->Snot[Ork_Second_B]</li>
				<li>NameSnot(p=5)->[Ork_First_A]snot</li>
				<li>NameSnot(p=1)->[Ork_Second_A]snot</li>
				
				<li>NameSnot(p=10)->Snot[Grot_First_B]</li>
				<li>NameSnot(p=7)->Snot[Grot_Second_B]</li>
				<li>NameSnot(p=5)->[Grot_First_A]snot</li>
				<li>NameSnot(p=1)->[Grot_Second_A]snot</li>
				
				<li>NameGod(p=1)->Gork</li>
				<li>NameGod(p=1)->Mork</li>
				
				<li>Klan(p=3)->[Klan_A] [Klan_B]</li>
			</rulesStrings>
			<rulesFiles>
				<li>Klan_A->NameBanks/OG_Greekskin_Ork_Names_Faction_A</li>
				<li>Klan_B->NameBanks/OG_Greekskin_Ork_Names_Faction_B</li>
				<li>Klan_C->NameBanks/OG_Greekskin_Ork_Names_Faction_C</li>
				<li>Kultrue->NameBanks/OG_Greekskin_Ork_Kulture</li>
				
				<li>Ork_Outpost->NameBanks/OG_Greekskin_Ork_Names_Outpost</li>
				
				<li>Ork_First->NameBanks/OG_Greekskin_Ork_Names_First</li>
				<li>Ork_First_A->NameBanks/OG_Greekskin_Ork_Names_First_A</li>
				<li>Ork_First_B->NameBanks/OG_Greekskin_Ork_Names_First_B</li>
				<li>Ork_Second->NameBanks/OG_Greekskin_Ork_Names_Last</li>
				<li>Ork_Second_A->NameBanks/OG_Greekskin_Ork_Names_Last_A</li>
				<li>Ork_Second_B->NameBanks/OG_Greekskin_Ork_Names_Last_B</li>
				<!--li>Ork_Nick->NameBanks/OG_Greekskin_Ork_Names_Nick</li-->
				
				<li>Ork_Warboss->NameBanks/OG_Greekskin_Ork_Names_Leader</li>
				<li>Ork_Title_Pre->NameBanks/OG_Greekskin_Ork_Titles_Leader_Prefix</li>
				<li>Ork_Title_Post->NameBanks/OG_Greekskin_Ork_Titles_Leader_Postfix</li>
				
				<li>Grot_First_A->NameBanks/OG_Greekskin_Grot_Names_First_A</li>
				<li>Grot_First_B->NameBanks/OG_Greekskin_Grot_Names_First_B</li>
				<li>Grot_Second_A->NameBanks/OG_Greekskin_Grot_Names_Second_A</li>
				<li>Grot_Second_B->NameBanks/OG_Greekskin_Grot_Names_Second_B</li>
				<li>Grot_Nick->NameBanks/OG_Greekskin_Grot_Names_Nick</li>
				
				<li>Squig->NameBanks/OG_Greekskin_Ork_Names_Last</li>
				<li>Squig_A->NameBanks/OG_Greekskin_Ork_Names_Squig_A</li>
				<li>Squig_B->NameBanks/OG_Greekskin_Ork_Names_Squig_B</li>
			</rulesFiles>
			<rulesRaw>
				<li Class="AdeptusMechanicus.Rule_Current_World">
					<keyword>CurrentWorld</keyword>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>

	<!-- ========================= Orkoid Kulture Utility ========================= -->
	
	<RulePackDef>
		<defName>NamerOrkoidKulture</defName>
		<include>
			<li>GlobalUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<!-- The system will feed one (and only one) of these cases into this namer:
					- key deity
					- symbol pack
					 These cases are separate, they won't mix.

					 The system will also feed a set of place_* keys correesponding to the ideo's foundation PlaceDef
				 -->

				<!-- From key deity -->
				<li>r_ideoName->[keyDeity]er's</li>
				<li>r_ideoName->[hyphenPrefix]-[keyDeity]er's</li>
				<li>r_ideoName->[keyDeity]er's [creed]</li>
				<li>r_ideoName->[creed] of [keyDeity]</li>
				<li>r_ideoAdjective->[keyDeity]ic</li>
				<li>r_memberName->[keyDeity]ist</li>
				<li>r_memberName->[keyDeity]ite</li>

				<!-- From symbol pack -->
				<li>r_ideoName(p=2)->[globalAdjective] [packIdeoName]</li>
				<li>r_ideoName(p=1.5,forcePrefix==False)->[hyphenPrefix]-[packIdeoName]</li>
				<li>r_ideoName->[memeAdjective] [packIdeoName]</li>
				<li>r_ideoName(p=0.5)->[memeAdjective] [creed]</li>
				<li>r_ideoName(p=0.2)->[creed] of [packTheme]</li>
				<li>r_ideoAdjective(priority=1)->[packAdjective]</li>
				<li>r_memberName->[packMember]</li>

				<!-- Generic content -->

				<li>creed(priority=1)->[memeCreed]</li>
				<li>creed->[genericCreed]</li>

				<li>genericCreed->creed</li>
				<li>genericCreed->doctrine</li>
				<li>genericCreed->way</li>
				<li>genericCreed->path</li>

				<li>globalAdjective->[Color]</li>
				<li>globalAdjective->New</li>
				<li>globalAdjective->Full</li>
				<li>globalAdjective->Old</li>
				<li>globalAdjective->Ancient</li>
				<li>globalAdjective->Historical</li>
				<li>globalAdjective->Revised</li>
				<li>globalAdjective->Reformed</li>
				<li>globalAdjective->Hard</li>
				<li>globalAdjective->Fundamental</li>
				<li>globalAdjective->True</li>
				<li>globalAdjective->Folk</li>
				<li>globalAdjective->Reformed</li>
				<li>globalAdjective->[place_adjective]</li>

				<li>hyphenPrefix->[globalHyphenPrefix]</li>
				<li>hyphenPrefix->[memeHyphenPrefix]</li>

				<li>globalHyphenPrefix(p=4)->neo</li>
				<li>globalHyphenPrefix(p=2)->post</li>
				<li>globalHyphenPrefix->nova</li>
				<li>globalHyphenPrefix->archaeo</li>
				<li>globalHyphenPrefix->hyper</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>OrkoidSpacer</defName>
		<rulePack>
			<rulesStrings>
				<li>globalAdjective->Spacer</li>
				<li>globalAdjective->Star</li>

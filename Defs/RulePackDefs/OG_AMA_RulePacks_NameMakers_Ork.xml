	<!-- Standard -->
	<RulePackDef>
		<defName>OG_Ork_NamerFaction</defName>
		<include>
			<li>OG_Orkoid_NameUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name(p=6)->[Kulture] [Klan_B] of [Klan_C]</li>
				<li>name(p=6)->[Kulture] [Klan_B] of [LeaderName]</li>
				<li>name(p=4)->[LeaderName]'z [Klan_B] of [Kulture]</li>
				<li>name(p=2)->[Klan_A] [Klan_B] of [Klan_C]</li>
				
				<!--li>name->[Waaagh] [LeaderName]</li-->
				
				<li>LeaderName(p=4)->[Ork_Warboss]</li>
				<li>LeaderName(p=2)->[Ork_Title_Pre] [Ork_Warboss]</li>
				<li>LeaderName(p=2)->[Ork_Warboss] [Ork_Title_Post]</li>
				<li>LeaderName(p=1)->[Ork_Title_Pre] [Ork_Warboss] [Ork_Title_Post]</li>
				
				<!--
				<li>name-> of da [OGOrkClans1]</li>
				
				<li>name-> of da [OGOrkClans1]</li>
				-->
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	

  <!-- Waaagh -->
	<RulePackDef>
		<defName>OG_Ork_NamerFaction_Waaagh</defName>
		<include>
			<li>OG_Orkoid_NameUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
			<li>name->[Waaagh] [LeaderName]</li>
			<li>Waaagh->Waaagh!</li>
			
			<li>LeaderName->[Ork_Warboss]</li>
			<li>LeaderName->[Ork_Warboss] [Ork_Title_Post]</li>
			<li>LeaderName->[Ork_Title_Pre] [Ork_Warboss]</li>
			<li>LeaderName->[Ork_Title_Pre] [Ork_Warboss] [Ork_Title_Post]</li>
			<!--
			<li>name-> of da [OGOrkClans1]</li>
			
			<li>name-> of da [OGOrkClans1]</li>
			-->
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
  <!-- Refugee -->
	<RulePackDef>
		<defName>OG_Ork_NamerFaction_Refugee</defName>
		<rulePack>
			<rulesStrings>

				<li>r_name->[adj] [group]</li>

				<li>group->survivaz</li>
				<li>group->gitz</li>
				<li>group->runtz</li>
				<li>group->kastawayz</li>
				<li>group->mob</li>
				<li>group->roamaz</li>
				<li>group->wandaraz</li>
				<li>group->travelaz</li>
				<li>group->nomadaz</li>

				<li>adj->'aggard</li>
				<li>adj->knackered</li>
				<li>adj->abandoned</li>
				<li>adj->exhausted</li>
				<li>adj->lost</li>
				<li>adj->battered</li>
				<li>adj->tough</li>
				<li>adj->dogged</li>
				<li>adj->tenacious</li>

			</rulesStrings>
		</rulePack>
	</RulePackDef>
  

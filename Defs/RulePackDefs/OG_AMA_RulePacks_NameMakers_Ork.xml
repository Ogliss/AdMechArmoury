      <rulesStrings>

        <li>r_name->[adj] [group]</li>

        <li>group->survivors</li>
        <li>group->refugees</li>
        <li>group->runaways</li>
        <li>group->castaways</li>
        <li>group->trekkers</li>
        <li>group->roamers</li>
        <li>group->wanderers</li>
        <li>group->travelers</li>
        <li>group->nomads</li>

        <li>adj->weary</li>
        <li>adj->drained</li>
        <li>adj->tired</li>
        <li>adj->exhausted</li>

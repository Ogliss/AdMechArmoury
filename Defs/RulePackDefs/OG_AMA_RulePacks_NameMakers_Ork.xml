			<rulesStrings>
				<li>name->[Klan_A] [Klan_B] [Klan_C]</li>
				<li>name-> of da [Klan_A]</li>
				<li>name-> of da [Klan_A]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	

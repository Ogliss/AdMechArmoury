<Defs>
	<RulePackDef>
		<defName>OG_NamerFaction_Ork</defName>
		<rulePack>
		  <rulesStrings>
			<li>name(p=6)->[Kulture] [OGOrkClans2] of [OGOrkClans3]</li>
			<li>name(p=6)->[Kulture] [OGOrkClans2] of [LeaderName]</li>
			<li>name(p=4)->[LeaderName]'z [OGOrkClans2] of [Kulture]</li>
			<li>name(p=2)->[OGOrkClans1] [OGOrkClans2] of [OGOrkClans3]</li>
			
			<!--li>name->[Waaagh] [LeaderName]</li-->
			
			<li>LeaderName(p=4)->[Leader]</li>
			<li>LeaderName(p=2)->[PreLeader] [Leader]</li>
			<li>LeaderName(p=2)->[Leader] [PostLeader]</li>
			<li>LeaderName(p=1)->[PreLeader] [Leader] [PostLeader]</li>
			<!--
			<li>name-> of da [OGOrkClans1]</li>
			
			<li>name-> of da [OGOrkClans1]</li>
			-->
		  </rulesStrings>
		  <rulesRaw>
			<li Class="Rule_File">
				<keyword>Kulture</keyword>
				<path>NameBanks/OG_Ork_Kulture</path>
			</li>
			<li Class="Rule_File">
				<keyword>Leader</keyword>
				<path>NameBanks/OG_Ork_Leader_Names</path>
			</li>
			<li Class="Rule_File">
				<keyword>PreLeader</keyword>
				<path>NameBanks/OG_Ork_Leader_Titles_Prefix</path>
			</li>
			<li Class="Rule_File">
				<keyword>PostLeader</keyword>
				<path>NameBanks/OG_Ork_Leader_Titles_Postfix</path>
			</li>
			<li Class="Rule_File">
				<keyword>OGOrkClans1</keyword>
				<path>NameBanks/OG_Ork_Faction_Names_A</path>
			</li>
			<li Class="Rule_File">
				<keyword>OGOrkClans2</keyword>
				<path>NameBanks/OG_Ork_Faction_Names_B</path>
			</li>
			<li Class="Rule_File">
				<keyword>OGOrkClans3</keyword>
				<path>NameBanks/OG_Ork_Faction_Names_C</path>
			</li>
		  </rulesRaw>
		</rulePack>
	</RulePackDef>
	

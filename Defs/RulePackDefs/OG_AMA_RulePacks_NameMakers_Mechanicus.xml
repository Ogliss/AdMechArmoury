<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>OG_Mechanicus_NamerFaction</defName>
		<include>
			<li>OG_Mechanicus_NameUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name->[Mechanicus_HomeWorld] [Ordinal] Legio</li>
				<!--
				<li>r_name->[nearnameinitial] [political_union_outlander]</li>
				<li>r_name->[nearnameinitial] [political_adjective] [political_union_outlander]</li>
				<li>r_name->[political_union_outlander] of [nearnameinitial]</li>
				<li>r_name->[political_adjective] [political_union_outlander] of [nearnameinitial]</li>
				-->
			</rulesStrings>
		</rulePack>
	</RulePackDef>
  
	<RulePackDef>
		<defName>OG_Mechanicus_NamerBases</defName>
		<include>
		  <li>OG_Mechanicus_NameUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[Mechanicus_Outpost]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>OG_Mechanicus_NamerPerson</defName>
		<include>
			<li>OG_Mechanicus_NameUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=10)->[Mechanicus_Name]</li>
				<li>r_name(p=10)->[Mechanicus_Name] `[MaleNick]`</li>
				<li>r_name(p=7)->[GreekLetter] [SingleDigit]-[Mechanicus_SingleSyllable]</li>
				<li>r_name(p=7)->[GreekLetter]-[GreekLetter] [SingleDigit][SingleDigit]</li>
				<li>r_name(p=7)->[Mechanicus_SingleSyllable] [Mechanicus_TriSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_SingleSyllable] [Mechanicus_QuadSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_DualSyllable] [Mechanicus_TriSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_DualSyllable] [Mechanicus_QuadSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_DualSyllable] [Mechanicus_SingleSyllable]-[Mechanicus_SingleSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_TriSyllable] [Mechanicus_SingleSyllable]-[Mechanicus_SingleSyllable]</li>
				<li>r_name(p=5)->[Capital]-[Capital] [Capital][SingleDigit][Capital]/[Capital][SingleDigit][Capital]</li>
				<li>r_name(p=5)->[Mechanicus_DualSyllable] [SingleDigit][SingleDigit]/[SingleDigit] [Mechanicus_DualSyllable]</li>
				<li>r_name(p=4)->[MaleFirst]</li>
				<li>r_name(p=4)->[MaleFirst] [MaleSecond]</li>
				<li>r_name(p=0.2)->[MaleFirst] `[MaleNick]`</li>
				<li>r_name(p=0.2)->[MaleFirst] `[MaleNick]` [MaleSecond]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>OG_Mechanicus_NamerPersonFemale</defName>
		<include>
			<li>OG_Mechanicus_NameUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=10)->[Mechanicus_Name]</li>
				<li>r_name(p=10)->[Mechanicus_Name] `[FemaleNick]`</li>
				<li>r_name(p=7)->[GreekLetter] [SingleDigit]-[Mechanicus_SingleSyllable]</li>
				<li>r_name(p=7)->[GreekLetter]-[GreekLetter] [SingleDigit][SingleDigit]</li>
				<li>r_name(p=7)->[Mechanicus_SingleSyllable] [Mechanicus_TriSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_SingleSyllable] [Mechanicus_QuadSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_DualSyllable] [Mechanicus_TriSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_DualSyllable] [Mechanicus_QuadSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_DualSyllable] [Mechanicus_SingleSyllable]-[Mechanicus_SingleSyllable]</li>
				<li>r_name(p=7)->[Mechanicus_TriSyllable] [Mechanicus_SingleSyllable]-[Mechanicus_SingleSyllable]</li>
				<li>r_name(p=5)->[Capital]-[Capital] [Capital][SingleDigit][Capital]/[Capital][SingleDigit][Capital]</li>
				<li>r_name(p=5)->[Mechanicus_DualSyllable] [SingleDigit][SingleDigit]/[SingleDigit] [Mechanicus_DualSyllable]</li>
				<li>r_name(p=4)->[FemaleFirst]</li>
				<li>r_name(p=4)->[FemaleFirst] [FemaleSecond]</li>
				<li>r_name(p=0.2)->[FemaleFirst] `[FemaleNick]`</li>
				<li>r_name(p=0.2)->[FemaleFirst] `[FemaleNick]` [FemaleSecond]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
  
	<RulePackDef>
		<defName>OG_Mechanicus_NameUtility</defName>
		<rulePack>
			<rulesStrings>
				<!--li>WordMechanicus(p=4)->[SylB][SylB]</li>
				<li>WordMechanicus(p=2)->[SylB][SylB][SylB]</li>
				<li>WordMechanicus(p=0.2)->[SylB][SylB][SylB][SylB]</li-->
				
				<li>MaleFirst->[Mechanicus_Male_First]</li>
				<!--li>MaleFirst->[Mechanicus_Male_First_A][Mechanicus_Male_First_B]</li-->
				<!--li>MaleFirst->[Mechanicus_Unisex_First]</li-->

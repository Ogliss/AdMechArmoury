				<li>MaleNick->[Mechanicus_Male_Nick]</li>
				<!--li>MaleNick->[Mechanicus_Male_Nick_A] [Mechanicus_Male_Nick_B]</li-->
				<li>MaleNick->[Mechanicus_Unisex_Nick]</li>
				<!--li>MaleNick->[Mechanicus_Unisex_Nick_A] [Mechanicus_Unisex_Nick_B]</li-->
				
				
				<li>FemaleFirst->[Mechanicus_Female_First]</li>
				<!--li>FemaleFirst->[Mechanicus_Female_First_A][Mechanicus_Female_First_B]</li-->
				<!--li>FemaleFirst->[Mechanicus_Unisex_First]</li-->
				<!--li>FemaleFirst->[Mechanicus_Unisex_First_A][Mechanicus_Unisex_First_B]</li-->
				
				<!--li>FemaleSecond->[Mechanicus_Female_Last]</li-->
				<!--li>FemaleSecond->[Mechanicus_Female_Last_A][Mechanicus_Female_Last_B]</li-->
				<li>FemaleSecond->[Mechanicus_Unisex_Last]</li>
				<!--li>FemaleSecond->[Mechanicus_Unisex_Last_A][Mechanicus_Unisex_Last_B]</li-->
				
				<li>FemaleNick->[Mechanicus_Female_Nick]</li>
				<!--li>FemaleNick->[Mechanicus_Female_Nick_A] [Mechanicus_Female_Nick_B]</li-->
				<li>FemaleNick->[Mechanicus_Unisex_Nick]</li>
				<!--li>FemaleNick->[Mechanicus_Unisex_Nick_A] [Mechanicus_Unisex_Nick_B]</li-->
				
				<!-- Unisex Names -->
				<li>Mechanicus_Name->[SingleDigit][SingleDigit]-[Mechanicus_SingleSyllable]</li>
				<li>Mechanicus_Name->[Capital][Capital]-[SingleDigit][SingleDigit][SingleDigit]</li>
				<li>Mechanicus_Name->[Capital]-[SingleDigit][SingleDigit][SingleDigit]</li>
				<li>Mechanicus_Name->[Mechanicus_DualSyllable]-[GreekLetter]-[SingleDigit]</li>
				<li>Mechanicus_Name->[Mechanicus_SingleSyllable][Mechanicus_Extra][Mechanicus_DualSyllable]</li>
				<li>Mechanicus_Name->[Mechanicus_DualSyllable][Mechanicus_Extra][Mechanicus_SingleSyllable]</li>
				<li>Mechanicus_Name->[Mechanicus_TriSyllable][Mechanicus_Extra][Mechanicus_SingleSyllable]</li>
				<li>Mechanicus_Name->[Mechanicus_TriSyllable][Mechanicus_Extra][Mechanicus_DualSyllable]</li>
				<li>Mechanicus_Name->[Mechanicus_QuadSyllable][Mechanicus_Extra][Mechanicus_SingleSyllable]</li>
				<li>Mechanicus_Name->[Mechanicus_QuadSyllable][Mechanicus_Extra][Mechanicus_DualSyllable]</li>
				
				<li>Mechanicus_SingleSyllable->[SingleSyllableA][SingleSyllableB]</li>
				<li>Mechanicus_DualSyllable->[DualSyllableA][DualSyllableB]</li>
				<li>Mechanicus_TriSyllable->[TriSyllableA][TriSyllableB][TriSyllableC]</li>
				<li>Mechanicus_QuadSyllable->[QuadSyllableA][QuadSyllableB][QuadSyllableC][QuadSyllableD]</li>
				
				<li>Mechanicus_Extra(p=5)-> </li>
				<li>Mechanicus_Extra(p=1)->-</li>
				<li>Mechanicus_Extra(p=1)-> van </li>
				<li>Mechanicus_Extra(p=1)-> van der </li>
				
				<li>Mechanicus_Title(p=1)-> [TitleA] [TitleB] of the [TitleC] [TitleD]</li>
			</rulesStrings>
			<rulesFiles>
				<li>Mechanicus_HomeWorld->NameBanks/OG_Imperial_Names_ForgeWorlds</li>
				<li>Mechanicus_Outpost->NameBanks/OG_Imperial_Names_Outpost</li>
				<!-- Male Names -->
				<li>Mechanicus_Male_First->NameBanks/OG_Imperial_Mechanicus_Names_First_Male</li>
				<!--li>Mechanicus_Male_First_A->NameBanks/OG_Imperial_Mechanicus_Names_First_Male_A</li-->
				<!--li>Mechanicus_Male_First_B->NameBanks/OG_Imperial_Mechanicus_Names_First_Male_B</li-->
				<!--li>Mechanicus_Male_Last->NameBanks/OG_Imperial_Mechanicus_Names_Last_Male</li-->
				<!--li>Mechanicus_Male_Last_A->NameBanks/OG_Imperial_Mechanicus_Names_Last_Male_A</li-->
				<!--li>Mechanicus_Male_Last_B->NameBanks/OG_Imperial_Mechanicus_Names_Last_Male_B</li-->
				<li>Mechanicus_Male_Nick->NameBanks/OG_Imperial_Mechanicus_Names_Nick_Male</li>
				<!--li>Mechanicus_Male_Nick_A->NameBanks/OG_Imperial_Mechanicus_Names_Nick_Male_A</li-->
				<!--li>Mechanicus_Male_Nick_B->NameBanks/OG_Imperial_Mechanicus_Names_Nick_Male_B</li-->
				<!-- Female Names -->
				<li>Mechanicus_Female_First->NameBanks/OG_Imperial_Mechanicus_Names_First_Female</li>
				<!--li>Mechanicus_Female_First_A->NameBanks/OG_Imperial_Mechanicus_Names_First_Female_A</li-->
				<!--li>Mechanicus_Female_First_B->NameBanks/OG_Imperial_Mechanicus_Names_First_Female_B</li-->
				<!--li>Mechanicus_Female_Last->NameBanks/OG_Imperial_Mechanicus_Names_Last_Female</li-->
				<!--li>Mechanicus_Female_Last_A->NameBanks/OG_Imperial_Mechanicus_Names_Last_Female_A</li-->
				<!--li>Mechanicus_Female_Last_B->NameBanks/OG_Imperial_Mechanicus_Names_Last_Female_B</li-->
				<li>Mechanicus_Female_Nick->NameBanks/OG_Imperial_Mechanicus_Names_Nick_Female</li>
				<!--li>Mechanicus_Female_Nick_A->NameBanks/OG_Imperial_Mechanicus_Names_Nick_Female_A</li-->
				<!--li>Mechanicus_Female_Nick_B->NameBanks/OG_Imperial_Mechanicus_Names_Nick_Female_B</li-->
				<!-- Unisex Names -->
				<!--li>Mechanicus_Unisex_First->NameBanks/OG_Imperial_Mechanicus_Names_First_Unisex</li-->

				<muzzleFlashScale>10</muzzleFlashScale>
				<barrelLength>0.6</barrelLength>
				<barrelOffset>0.0</barrelOffset>
				<targetParams>
					<canTargetLocations>true</canTargetLocations>
				</targetParams>
			</li>
		</verbs>
		<tools>
			<li>
				<label>stock</label>
				<capacities>
				<li>Blunt</li>
				</capacities>
				<power>9</power>
				<cooldownTime>1.8</cooldownTime>
			</li>
			<li>
				<label>barrel</label>

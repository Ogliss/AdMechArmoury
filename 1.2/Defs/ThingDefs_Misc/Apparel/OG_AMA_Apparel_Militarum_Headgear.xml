	<!-- ========================= Imperial Guard - Field Cap ========================== -->
	<ThingDef ParentName="OG_Imperial_Apparel_Headgear" Name="FieldHat">
		<defName>OGIG_Apparel_FieldCap</defName>
		<label>field cap</label>
		<description>A hat worn by lower ranking Imperial officers.</description>
		<graphicData>
			<texPath>Things/Item/Apparel/Imperial/IG/IG_Fieldcap/IG_FieldCap</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<WorkToMake>1800</WorkToMake>
			<Mass>0.05</Mass>
			<StuffEffectMultiplierArmor>0.21</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.45</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>0.4</EquipDelay>
			<Bulk MayRequire="CETeam.CombatExtended">0.3</Bulk>
			<WornBulk MayRequire="CETeam.CombatExtended">0.4</WornBulk>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.0</MoveSpeed>
			<SocialImpact>0.025</SocialImpact>
			<MentalBreakThreshold>-0.025</MentalBreakThreshold>
		</equippedStatOffsets>
		<generateCommonality>2</generateCommonality>
		<apparel>
			<useWornGraphicMask>true</useWornGraphicMask>
			<wornGraphicPath>Things/Item/Apparel/Imperial/IG/IG_Fieldcap/IG_FieldCap</wornGraphicPath>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<layers>
				<li MayRequire="CETeam.CombatExtended">OnHead</li>
				<li MayRequire="CETeam.CombatExtended">MiddleHead</li>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>OGIGHeadgear</li>
				<li>OGImperialGuard</li>
				<li>OGIGFieldCap</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<only>RGBA(1.00,1.00,1.00,1)</only>
				</li>
				<li>
					<only>RGBA(0.95,0.95,0.95,1)</only>
				</li>
				<li>
					<only>RGBA(0.90,0.90,0.90,1)</only>
				</li>
				<li>
					<only>RGBA(0.85,0.85,0.85,1)</only>
				</li>
				<li>
					<only>RGBA(0.80,0.80,0.80,1)</only>
				</li>
				<li>
					<only>RGBA(0.75,0.75,0.75,1)</only>
				</li>
				<li>
					<only>RGBA(0.70,0.70,0.70,1)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>


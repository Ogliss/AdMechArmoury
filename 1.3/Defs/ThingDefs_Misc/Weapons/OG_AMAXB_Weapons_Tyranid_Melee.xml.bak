	<!--==================== Tyranid - Carnifex Weapons =========================-->
	<ThingDef MayRequire="Ogliss.AdMech.Xenobiologis" ParentName="OG_Tyranid_MeleeWeapon_Sharp_Quality">
		<defName>OGTY_Melee_Carnifex_STCC</defName>

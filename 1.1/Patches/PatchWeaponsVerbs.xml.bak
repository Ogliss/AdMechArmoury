<?xml version="1.0" encoding="utf-8" ?>
<Patch>
 <!-- ====================== Gun_BoltGun ===================== -->	
	<Operation Class="PatchOperationSequence">
	<success>Always</success>
		<operations>	
			<li Class="PatchOperationTest">
				<xpath>/Defs/ThingDef[defName = "Gun_BoltGun"]</xpath>

			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "Gun_BoltGun"]</xpath>
				<value>
				<thingClass>AdeptusMechanicus.ThingDef_GunOG</thingClass>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "Gun_BoltGun"]/verbs</xpath>
				<value><verbs>
				
		<li Class="AdeptusMechanicus.VerbPropertiesOG">
			<verbClass>AdeptusMechanicus.Verb_ShootOG</verbClass>
			<rapidfire>true</rapidfire>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>OGI_Bullet_BoltStd</defaultProjectile>
			<warmupTime>0.30</warmupTime>
			<range>31</range>
			<ticksBetweenBurstShots>10</ticksBetweenBurstShots>
			<burstShotCount>3</burstShotCount>
			<soundCast>BoltSound</soundCast>
			<soundCastTail>GunTail_Heavy</soundCastTail>
			<muzzleFlashScale>14</muzzleFlashScale>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
			</targetParams>
      </li>
				</verbs></value>
			</li>
		</operations>
	</Operation>


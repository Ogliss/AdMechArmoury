	<!-- ==================== Mod - Base Dark Eldar Melee Weapon Template ========================= -->
	<ThingDef Name="OG_DarkEldar_MeleeWeapon_Base" Abstract="True" ParentName="OG_Weapon_Base">
    <techLevel>Industrial</techLevel>
		<smeltable>true</smeltable>
    <thingCategories>
      <li>OGDEMelee</li>
    </thingCategories>
    <weaponTags>
      <li>DarkEldarMelee</li>
      <li>DarkEldarWeapon</li>
    </weaponTags>

    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Smith</soundWorking>
      <recipeUsers>
	<!--
        <li>OG_DarkEldar_TableMachining</li>
	-->
        <li>OG_Eldar_TableMachining</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedWeapon</unfinishedThingDef>
		<researchPrerequisites>
			<li>OG_Aeldari_Tech_Base</li>
			<li>OG_DarkEldar_Tech_Base_T1</li>
		</researchPrerequisites>
    </recipeMaker>
    <inspectorTabs>
      <li>ITab_Art</li>
    </inspectorTabs>
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<li Class="CompProperties_Art">
				<nameMaker>NamerArtWeaponMelee</nameMaker>
				<descriptionMaker>ArtDescription_WeaponMelee</descriptionMaker>
				<minQualityForArtistic>Excellent</minQualityForArtistic>
			</li>
		</comps>
	</ThingDef>

	<!-- ==================== Mod - Base Dark Eldar Melee Weapon Sharp Template ========================= -->

      <unfinishedThingDef>UnfinishedGun</unfinishedThingDef>
		<researchPrerequisites>
			<li>Gunsmithing</li>
			<li>OG_Aeldari_Tech_Base</li>
		</researchPrerequisites>
    </recipeMaker>
	</ThingDef>
   

      <tags>
        <li>OGImperialGuard</li>
        <li>OGIGArmour</li>
        <li>OGIGFlak</li>
        <li>OGIGFlakChest</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>

					<padTexPath>Things/Item/Apparel/Common/Cloak/Cloak</padTexPath>
					<commonality>10</commonality>
					<northtop>PostHead</northtop>
					<southtop>Shell</southtop>
					<easttop>Shell</easttop>
					<westtop>Shell</westtop>
				</li>
			</PauldronEntries>
			</li>
		</comps>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>OG_Apparel_Tribal_Cloak</defName>
		<label>tribalcloak</label>
		<description>A full-body garment crafted using neolithic tools. While it may look primitive, it is effective at insulating the wearer.</description>
		<graphicData>
			<texPath>Things/Item/Apparel/Common/Tribal_Cloak/Tribal_Cloak</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<!--
			<shaderType>CutoutComplex</shaderType> 
			-->
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<generateCommonality>0.25</generateCommonality>
		<costStuffCount>60</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<tradeability>Sellable</tradeability>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>1800</WorkToMake>
			<Mass>0.4</Mass>
			<StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.55</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.55</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Neck</li>
				<li>Legs</li>
			</bodyPartGroups>
			<!--wornGraphicPath>Things/Item/Apparel/Common/Tribal_Cloak/Tribal_Cloak</wornGraphicPath-->
			<layers>
				<li>ShellExtra</li>
			</layers>
			<tags>
				<li>Neolithic</li>
				<li>TribalCloak</li>
				<li>Cloak</li>
			</tags>
		</apparel>
		<comps>
			<li Class="AdeptusMechanicus.CompProperties_PauldronDrawer">
			<compClass>AdeptusMechanicus.CompPauldronDrawer</compClass>
			<drawAll>true</drawAll>
			<PauldronEntries>
				<li>
					<shoulderPadType>SouthOnly</shoulderPadType>
					<shaderType>CutoutComplex</shaderType>
					<padTexPath>Things/Item/Apparel/Common/Cloak/Backbit/Cloak</padTexPath>
					<commonality>10</commonality>
					<southtop>Utility_South</southtop>
				</li>
				<li>
					<shoulderPadType>Backpack</shoulderPadType>
					<shaderType>CutoutComplex</shaderType>
					<padTexPath>Things/Item/Apparel/Common/Tribal_Cloak/Tribal_Cloak</padTexPath>
					<commonality>10</commonality>
					<northtop>PostHead</northtop>
					<southtop>Shell</southtop>
					<easttop>Shell</easttop>
					<westtop>Shell</westtop>
				</li>
			</PauldronEntries>
			</li>
		</comps>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>
		<!--
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(0.4,0.3,0.15)</only>
				</li>
				<li>
					<weight>15</weight>
					<only>(0.6,0.45,0.18)</only>
				</li>
				<li>
					<weight>20</weight>
					<only>(0.8,0.6,0.23)</only>
				</li>
			</options>
		</colorGenerator>
		-->
	</ThingDef>
	
	

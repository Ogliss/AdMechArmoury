<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--OG==================== Adeptus Mechanicus Armoury Mod - Research Astartes =========================OG-->

	<ResearchProjectDef ParentName="OG_Imperial_ReseachBase">
		<defName>OG_Astartes_Tech_Base_T1</defName> 
		<label>Astartes Neophytes</label>
		<description>Grants you access to STC templates used by the Adeptus Arbites</description>
		<baseCost>4000</baseCost>
		<prerequisites>
			<li>OG_Imperial_Tech_Base_T0</li>
		</prerequisites>
		<techLevel>Industrial</techLevel>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
		<researchViewX>1.00</researchViewX>
		<researchViewY>3.50</researchViewY>
	</ResearchProjectDef>
	
	<ResearchProjectDef ParentName="OG_Imperial_ReseachBase">
		<defName>OG_Astartes_Tech_Base_T2</defName> 
		<label>Astartes Scout</label>
		<description>Grants you access to STC templates used by the Adeptus Militarum</description>
		<baseCost>6000</baseCost>
		<prerequisites>
			<li>OG_Astartes_Tech_Base_T1</li>
		</prerequisites>
		<techLevel>Industrial</techLevel>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
		<requiredResearchFacilities>
			<li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<researchViewX>3.00</researchViewX>
		<researchViewY>3.50</researchViewY>
	</ResearchProjectDef>
	
	<ResearchProjectDef ParentName="OG_Imperial_ReseachBase">
		<defName>OG_Astartes_Tech_Base_T3</defName> 
		<label>Astartes Brother</label>
		<description>Grants you access to STC templates used to create finest weapons and wargear in the Departmento Munitorum's arsenal, Tempestus Scions are equipped to carry out missions beyond the capabilities of the rank-and-file Guardsman.</description>
		<baseCost>8000</baseCost>
		<prerequisites>
			<li>Fabrication</li>
			<li>OG_Astartes_Tech_Base_T2</li>
		</prerequisites>
		<techLevel>Industrial</techLevel>
		<requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
		<requiredResearchFacilities>
			<li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<researchViewX>5.00</researchViewX>
		<researchViewY>3.50</researchViewY>
	</ResearchProjectDef>

</Defs>